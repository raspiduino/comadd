{
	"manifest_version": 2,
	"name": "ComAdd Extension",
	"description": "Track the web time",
	"version": "1.0.0",
	"author": "Raspiduino",
	"browser_action": {
		"default_icon": "icon.ico",
		"default_title": "Track the web time"
	},
	"permissions": ["tabs", "activeTab", "http://127.0.0.1:5000/*", "storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}